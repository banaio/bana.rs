{
  "[Log]": {
    "editor.fontSize": 14
  },
  "[git-commit]": {
    "editor.rulers": [
      50,
      72
    ]
  },
  "[javascript]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[json]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.wordWrap": "on"
  },
  "[jsonc]": {
    "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.wordWrap": "on"
  },
  "[markdown]": {
    "editor.quickSuggestions": false,
    "editor.wordWrap": "on"
  },
  "[shellscript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  // "[yaml]": {
  //   // "editor.autoIndent": "full"
  // },
  "breadcrumbs.enabled": false,
  "cSpell.allowCompoundWords": true,
  "cSpell.checkLimit": 9007199254740991,
  "cSpell.dictionaries": [
    "wordsEn",
    "wordsEnGb",
    "companies",
    "softwareTerms",
    "misc",
    "typescrip",
    "node",
    "php",
    "go",
    "python",
    "powershell",
    "html",
    "css"
  ],
  "cSpell.enableFiletypes": [
    "!.*.log"
  ],
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "python",
    "pug",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "cSpell.language": "en-GB",
  "cSpell.logLevel": "Error",
  "cSpell.maxDuplicateProblems": 2,
  "cSpell.maxNumberOfProblems": 5,
  "cSpell.minWordLength": 1,
  "cSpell.spellCheckDelayMs": 1000,
  "cSpell.words": [
    "Bana",
    "Marrakech",
    "kubernetes"
  ],
  "diffEditor.renderIndicators": true,
  "diffEditor.renderSideBySide": true,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true,
    "source.fixAll.tslint": true
  },
  "editor.codeLens": false,
  "editor.cursorBlinking": "blink",
  "editor.cursorSmoothCaretAnimation": false,
  "editor.cursorStyle": "block-outline",
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.fontFamily": "'Iosevka Fixed Extended','Iosevka-Extended','monospace', monospace",
  // "editor.fontFamily": "'Iosevka Fixed','Iosevka-Extended','monospace', monospace",
  "editor.fontLigatures": true,
  "editor.fontSize": 15,
  "editor.formatOnPaste": false,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "none",
  "editor.rulers": [
    80,
    100
  ],
  "editor.scrollBeyondLastLine": true,
  "editor.snippetSuggestions": "inline",
  "editor.wordWrap": "on",
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  // "eslint.alwaysShowStatus": true,
  // "eslint.enable": true,
  // "eslint.format.enable": true,
  // "eslint.onIgnoredFiles": "warn",
  // "eslint.packageManager": "yarn",
  // "eslint.probe": [
  //   "javascript",
  //   "javascriptreact",
  //   "typescript",
  //   "typescriptreact",
  //   "html",
  //   "vue",
  //   "markdown",
  //   "json",
  //   "jsonc"
  // ],
  // "eslint.run": "onSave",
  // "eslint.validate": [
  //   "javascript",
  //   "javascriptreact",
  //   "typescript",
  //   "typescriptreact",
  //   "html",
  //   "vue",
  //   "markdown",
  //   "json",
  //   "jsonc"
  // ],
  // "eslint.workingDirectories": [
  //   "${workspaceFolder}"
  // ],
  "explorer.autoReveal": "focusNoScroll",
  "explorer.compactFolders": false,
  "explorer.confirmDragAndDrop": true,
  "explorer.openEditors.visible": 3,
  "explorer.sortOrder": "type",
  "files.associations": {
    "*_profile": "shellscript",
    ".*ignore": "ignore",
    "\\..*profile": "shellscript",
    "\\..*rc": "shellscript",
    "^*_profile$": "shellscript",
    "^\\..*profile$": "shellscript",
    "^\\..*rc$": "shellscript"
  },
  "files.autoSave": "onFocusChange",
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.exclude": {
    "**/.cache": true,
    "**/.git": true,
    "**/.temp": true,
    "**/node_modules": true,
    "**/node_modules/*": true,
    "node_modules": true,
    "target": true,
    "target/": true,
    "target/**/*": true,
  },
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.watcherExclude": {
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/node_modules/*": true,
    "target": true,
    "target/": true,
    "target/**/*": true,
  },
  "markdownlint.run": "onSave",
  "problems.showCurrentInStatus": true,
  "scm.defaultViewMode": "tree",
  "search.actionsPosition": "right",
  "search.collapseResults": "alwaysCollapse",
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.sortOrder": "countDescending",
  "terminal.explorerKind": "external",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "editorBracketMatch.background": "#b71c1c",
    "editorBracketMatch.border": "#b71c1c",
    "editorCursor.foreground": "#ffff72",
    "editorIndentGuide.activeBackground": "#ff7961",
    "editorIndentGuide.background": "#ffff7245"
  },
  "workbench.commandPalette.preserveInput": true,
  "workbench.editor.closeOnFileDelete": false,
  "workbench.editor.focusRecentEditorAfterClose": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "medium",
  "workbench.panel.defaultLocation": "right",
  "workbench.settings.editor": "json",
  "workbench.settings.openDefaultSettings": false,
  "workbench.settings.useSplitJSON": false,
  "workbench.tree.indent": 8,
  "search.quickOpen.includeHistory": true,
  "editor.stablePeek": true,
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 3,
  "yaml.format.singleQuote": true,
  "yaml.format.bracketSpacing": true,
  "yaml.format.proseWrap": "never",
  "yaml.format.enable": true,
  "yaml.format.printWidth": 140,
  "yaml.validate": true,
  "yaml.hover": true,
  "yaml.completion": true,
  "yaml.schemaStore.enable": true,
  "yaml.schemas": {
    // "/": "https://github.com/SchemaStore/schemastore/blob/master/src/schemas/json/github-action.json",
    // "//": "https://github.com/SchemaStore/schemastore/blob/master/src/schemas/json/github-workflow.json",
    // "//.github/workflow_schemas/github-action.json": [
    //   "//.github/workflows/*.yml",
    //   "//.github/workflows/*.yaml",
    // ],
    ".github/workflow_schemas/github-workflow.json": [
      ".github/workflows/*.yml",
      ".github/workflows/*.yaml",
    ],
    // "//github-action": [
    //   "//.github/workflows/*.yml",
    //   "//.github/workflows/*.yaml",
    // ],
    // "//https://json.schemastore.org/github-action": [
    //   "//.github/workflows/*.yml",
    //   "//.github/workflows/*.yaml",
    // ],
    // "github-workflow": [
    //   ".github/workflows/*.yml",
    //   ".github/workflows/*.yaml",
    // ],
    // "https://json.schemastore.org/github-workflow": [
    //   ".github/workflows/*.yml",
    //   ".github/workflows/*.yaml",
    // ],
  },
  "yaml.trace.server": "verbose",
  "extensions.showRecommendationsOnlyOnDemand": true,
}
