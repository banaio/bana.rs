{
  "[Log]": {
    "editor.fontSize": 12
  },
  "[git-commit]": {
    "editor.rulers": [
      50,
      72
    ]
  },
  "[javascript]": {
    // "editor.defaultFormatter": "dbaeumer.vscode-eslint"
  },
  "[json]": {
    // "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.wordWrap": "on"
  },
  "[jsonc]": {
    // "editor.defaultFormatter": "dbaeumer.vscode-eslint",
    "editor.wordWrap": "on"
  },
  "[markdown]": {
    "editor.quickSuggestions": false,
    "editor.wordWrap": "on"
  },
  "[shellscript]": {
    "editor.insertSpaces": true,
    "editor.tabSize": 2
  },
  "breadcrumbs.enabled": false,
  "cSpell.allowCompoundWords": true,
  "cSpell.checkLimit": 9007199254740991,
  "cSpell.dictionaries": [
    "wordsEn",
    "wordsEnGb",
    "companies",
    "softwareTerms",
    "misc",
    "typescrip",
    "node",
    "php",
    "go",
    "python",
    "powershell",
    "html",
    "css"
  ],
  "cSpell.enableFiletypes": [
    "!.*.log"
  ],
  "cSpell.enabledLanguageIds": [
    "asciidoc",
    "c",
    "cpp",
    "csharp",
    "css",
    "git-commit",
    "go",
    "handlebars",
    "haskell",
    "html",
    "jade",
    "java",
    "javascript",
    "javascriptreact",
    "json",
    "jsonc",
    "latex",
    "less",
    "markdown",
    "php",
    "plaintext",
    "python",
    "pug",
    "restructuredtext",
    "rust",
    "scala",
    "scss",
    "text",
    "typescript",
    "typescriptreact",
    "yaml",
    "yml"
  ],
  "cSpell.language": "en-GB",
  "cSpell.logLevel": "Error",
  "cSpell.maxDuplicateProblems": 2,
  "cSpell.maxNumberOfProblems": 5,
  "cSpell.minWordLength": 1,
  "cSpell.spellCheckDelayMs": 1000,
  "cSpell.words": [
    "Bana",
    "Marrakech",
    "kubernetes"
  ],
  "diffEditor.renderIndicators": true,
  "diffEditor.renderSideBySide": true,
  "editor.accessibilitySupport": "off",
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true,
    "source.fixAll.stylelint": true,
    "source.fixAll.tslint": true
  },
  "editor.codeLens": false,
  "editor.cursorBlinking": "blink",
  "editor.cursorSmoothCaretAnimation": false,
  "editor.cursorStyle": "line-thin",
  "editor.cursorSurroundingLinesStyle": "all",
  "editor.fontSize": 15,
  "editor.fontFamily": " 'Iosevka', 'Iosevka-Extended', 'monospace', monospace ",
  "editor.fontFamily/": " 'Iosevka Fixed Extended', 'Iosevka', 'Iosevka Extended', 'Iosevka-Extended', 'monospace', monospace ",
  "editor.fontFamily//": " 'Kawkab Mono Light', 'Kawkab Mono', 'Kawkab Mono', 'Iosevka Fixed Extended', 'Iosevka-Extended', 'monospace', monospace ",
  "editor.fontWeight/": "normal",
  "editor.fontLigatures": true,
  "editor.formatOnPaste": false,
  "editor.minimap.enabled": false,
  "editor.renderLineHighlight": "all",
  "editor.renderWhitespace": "all",
  "editor.rulers": [
    80,
    100,
    120,
  ],
  "editor.scrollBeyondLastLine": true,
  "editor.snippetSuggestions": "inline",
  "editor.stablePeek": true,
  "editor.wordWrap": "on",
  "emmet.excludeLanguages": [],
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "vue-html": "html"
  },
  "emmet.showSuggestionsAsSnippets": true,
  "emmet.triggerExpansionOnTab": true,
  "explorer.confirmDragAndDrop": true,
  "explorer.openEditors.visible": 8,
  "explorer.sortOrder": "type",
  "extensions.showRecommendationsOnlyOnDemand": true,
  "files.associations": {
    "*_profile": "shellscript",
    ".*ignore": "ignore",
    "\\..*profile": "shellscript",
    "\\..*rc": "shellscript",
    "^*_profile$": "shellscript",
    "^\\..*profile$": "shellscript",
    "^\\..*rc$": "shellscript",
    "**/.cargo/config": "toml",
  },
  "files.autoSave": "onWindowChange",
  "files.encoding": "utf8",
  "files.eol": "\n",
  "files.insertFinalNewline": true,
  "files.trimFinalNewlines": true,
  "files.trimTrailingWhitespace": true,
  "files.exclude": {
    "**/.cache": true,
    "**/.git": true,
    "**/.temp": true,
    "**/node_modules": true,
    "**/node_modules/*": true,
    "node_modules": true,
    "target": true,
    "target/": true,
    "target/**/*": true
  },
  "files.watcherExclude": {
    "**/.cache": true,
    "**/.git": true,
    "**/.temp": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/node_modules/*": true,
    "target": true,
    "target/": true,
    "target/**/*": true
  },
  "search.exclude": {
    "**/node_modules": true,
    "**/bower_components": true,
    "**/*.code-search": true,
    "**/.cache": true,
    "**/.git": true,
    "**/.temp": true,
    "**/.git/objects/**": true,
    "**/.git/subtree-cache/**": true,
    "**/.hg/store/**": true,
    "**/node_modules/*": true,
    "target": true,
    "target/": true,
    "target/**/*": true
  },
  "markdownlint.run": "onSave",
  "problems.autoReveal": true,
  "problems.decorations.enabled": true,
  "outline.problems.badges": true,
  "outline.problems.colors": true,
  "outline.problems.enabled": true,
  "problems.showCurrentInStatus": true,
  "scm.defaultViewMode": "tree",
  "search.actionsPosition": "right",
  "search.collapseResults": "alwaysCollapse",
  "search.quickOpen.includeHistory": true,
  "search.showLineNumbers": true,
  "search.smartCase": true,
  "search.sortOrder": "countDescending",
  "terminal.explorerKind": "external",
  "window.titleBarStyle": "custom",
  "window.zoomLevel": 0,
  "workbench.colorCustomizations": {
    "editorBracketMatch.background": "#b71c1c",
    "editorBracketMatch.border": "#b71c1c",
    "editorCursor.foreground": "#ffff72",
    "editorIndentGuide.activeBackground": "#ff7961",
    "editorIndentGuide.background": "#ffff7245",
    "editor.lineHighlightBorder": "#ffff7230",
  },
  "workbench.commandPalette.preserveInput": true,
  "workbench.editor.closeOnFileDelete": false,
  "workbench.editor.focusRecentEditorAfterClose": true,
  "workbench.editor.highlightModifiedTabs": true,
  "workbench.editor.labelFormat": "medium",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 8,
  "workbench.panel.defaultLocation": "right",
  "workbench.settings.editor": "json",
  "workbench.settings.openDefaultSettings": false,
  "workbench.settings.useSplitJSON": false,
  "workbench.tree.indent": 8,
  "yaml.completion": true,
  "yaml.format.bracketSpacing": true,
  "yaml.format.enable": true,
  "yaml.format.printWidth": 100,
  "yaml.format.proseWrap": "never",
  "yaml.format.singleQuote": true,
  "yaml.hover": true,
  "yaml.schemaStore.enable": true,
  "yaml.schemas": {
    ".github/workflow_schemas/github-workflow.json": [
      ".github/workflows/*.yml",
      ".github/workflows/*.yaml"
    ]
  },
  "yaml.trace.server": "verbose",
  "yaml.validate": true,
  "workbench.view.alwaysShowHeaderActions": true,
  "workbench.editor.limit.perEditorGroup": true,
  "workbench.editor.revealIfOpen": true,
  "workbench.editor.titleScrollbarSizing": "large",
  "search.maintainFileSearchCache": true,
  "search.seedOnFocus": true,
  "workbench.commandPalette.history": 64,
  "workbench.list.horizontalScrolling": true,
  "workbench.list.keyboardNavigation": "simple",
  "workbench.list.smoothScrolling/": true,
  "workbench.quickOpen.preserveInput": true,
  "workbench.sash.size": 20,
  "workbench.tree.renderIndentGuides": "always",
  "editor.wordWrapColumn": 100,
  "editor.accessibilityPageSize": 1,
  "editor.autoClosingBrackets": "beforeWhitespace",
  "editor.autoClosingOvertype": "auto",
  "editor.columnSelection/": true,
  "editor.foldingStrategy": "indentation",
  "editor.parameterHints.cycle": true,
  "editor.scrollBeyondLastColumn": 32,
  "editor.smoothScrolling/": true,
  "editor.wrappingIndent": "indent",
  "editor.wrappingStrategy": "advanced",
  "editor.suggest.localityBonus": true,
  "editor.suggest.statusBar.visible": true,
  "editor.fastScrollSensitivity": 8,
  "editor.mouseWheelScrollSensitivity": 2,
  "terminal.integrated.rendererType": "canvas",
  "terminal.integrated.fontFamily/": "'Source Code Pro for Powerline', 'Iosevka Fixed Extended', 'Iosevka-Extended', 'monospace', monospace",
  "terminal.integrated.automationShell.linux": "/usr/bin/zsh",
  "terminal.integrated.automationShell.osx": "/usr/local/bin/zsh",
  "terminal.integrated.shell.linux": "/usr/bin/zsh",
  "terminal.integrated.shell.osx": "/usr/local/bin/zsh",
  "shellformat.path": "~/go/bin/shfmt",
  "editor.autoIndent": "advanced",
  "editor.hover.delay": 100,
  "editor.padding.top": 16,
  "editor.padding.bottom": 16,
  "search.searchEditor.reusePriorSearchConfiguration": true,
  "workbench.settings.enableNaturalLanguageSearch": false,
  "telemetry.enableTelemetry": false,
  "telemetry.enableCrashReporter": false,
  "workbench.enableExperiments": false,
  "workbench.editor.restoreViewState": true,
  "workbench.editor.centeredLayoutAutoResize/": true,
  "shellcheck.enableQuickFix": true,
  "shellcheck.run": "onSave",
  "shellcheck.useWorkspaceRootAsCwd": true,
  "shellcheck.executablePath": "shellcheck",
  "shellcheck.executablePath/": "/usr/local/bin/shellcheck",
}
